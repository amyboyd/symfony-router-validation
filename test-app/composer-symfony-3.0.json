{
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/TestAppKernel.php"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "3.0.*",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0.2",
        "phpunit/phpunit": "~4.8"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
